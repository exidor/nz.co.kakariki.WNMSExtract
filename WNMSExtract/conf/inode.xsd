<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation xml:lang="en">
   INode Schema for wnmsextract.nz.alcatel-lucent.com
   Copyright 2010 Alcatel-Lucent All rights reserved.
  </xs:documentation>
	</xs:annotation>
	<!-- 
++ 
-->
	<xs:element name="INode" type="RootINodeType"/>
	<!-- 
++ 
-->
	<xs:complexType name="RootINodeType">
		<xs:sequence>
			<xs:element name="id" type="xs:string"/>
			<xs:element name="timestamp" type="xs:string"/>
			<xs:sequence>
				<xs:element name="RncEquipment" type="RncEquipmentType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="RncEquipmentType">
		<xs:sequence>
			<xs:element name="INode" type="INodeType"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:string"/>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="INodeType">
		<xs:choice>
			<xs:sequence>
				<xs:element name="VS.CpuUtilAvg" type="xs:float"/>
				<xs:element name="VS.CpuUtilAvgMax" type="xs:float"/>
				<xs:element name="VS.CpuUtilAvgMin" type="xs:float"/>
			</xs:sequence>
			<xs:sequence>
				<xs:element name="AtmPort" type="AtmPortType" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:sequence>
				<xs:element name="Lp" type="LpType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="id" type="xs:string"/>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="AtmPortType">
		<xs:sequence>
			<xs:element name="VS.RxMaxCellRate" type="xs:integer"/>
			<xs:element name="VS.TxMaxCellRate" type="xs:integer"/>
			<xs:element name="VS.RxAvgCellRate" type="xs:float"/>
			<xs:element name="VS.TxAvgCellRate" type="xs:float"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:string"/>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="LpType">
		<xs:choice>
			<xs:sequence>
				<xs:element name="VS.CpuUtilAvg" type="xs:float"/>
				<xs:element name="VS.CpuUtilAvgMax" type="xs:float"/>
				<xs:element name="VS.CpuUtilAvgMin" type="xs:float"/>
			</xs:sequence>
			<xs:sequence>
				<xs:element name="Ap" type="ApType" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:sequence>
				<xs:element name="Ethernet" type="EthernetType" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:choice>
		<xs:attribute name="id" type="xs:int"/>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="ApType">
		<xs:sequence>
			<xs:element name="VS.APCpuUtilizationAvg" type="xs:float"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:string"/>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="EthernetType">
		<xs:sequence>
			<xs:element name="VS.avgRxUtilization" type="xs:float"/>
			<xs:element name="VS.avgTxUtilization" type="xs:float"/>
			<xs:element name="VS.maxRxUtilization" type="xs:float"/>
			<xs:element name="VS.maxTxUtilization" type="xs:float"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:string"/>
	</xs:complexType>
	<!-- 
++ 
-->
</xs:schema>
