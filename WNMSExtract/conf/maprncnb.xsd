<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation>
		<xs:documentation xml:lang="en">
   RNC 2 NodeB Mapping Schema for wnmsextract.nz.alcatel-lucent.com
   Copyright 2010 Alcatel-Lucent All rights reserved.
		  </xs:documentation>
	</xs:annotation>
	<!-- 
++ 
-->
	<xs:element name="MapRncNodeB" type="RncNodeBType"/>
	<!-- 
++ 
-->
	<xs:complexType name="RncNodeBType">
		<xs:sequence>
			<xs:element name="id" type="xs:string"/>
			<xs:element name="Rnc" type="RncType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="RncType">
		<xs:sequence>
			<xs:element name="NodeB" type="NodeBType" nillable="true" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="id" type="xs:string"/>
	</xs:complexType>
	<!-- 
++ 
-->
	<xs:complexType name="NodeBType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="id" type="xs:string"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<!-- 
++ 
-->
</xs:schema>
